        "script-loader": "^0.7.2",
        "style-loader": "^1.3.0",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard-scss": "^4.0.0",
        "stylelint-prettier": "^2.0.0",
        "stylelint-scss": "^4.2.0",
        "uglifyjs-webpack-plugin": "^2.1.1",
        "webpack": "^4.28.4",
        "webpack-cli": "^3.3.0",
